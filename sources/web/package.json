    },
    "browserslist": [
        "> 0.5%",
        "last 2 versions",
        "Firefox ESR",
        "not dead"
    ],
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "modules": false
                }
            ]
        ],
        "env": {
            "test": {
                "presets": [
                    [
                        "@babel/preset-env",
                        {
                            "targets": {
                                "node": "current"
                            }
                        }
                    ]
                ]
            }
        },
        "plugins": [
            "@babel/plugin-transform-runtime"
        ]
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "vue"
        ],
        "transform": {
            "^.+\\.js$": "babel-jest",
            "^.+\\.vue$": "vue-jest"
        },
        "transformIgnorePatterns": [
            "<rootDir>/node_modules/"
        ],
        "verbose": true,
        "coverageDirectory": "<rootDir>/build/coverage/javascript/",
        "collectCoverageFrom": [
            "<rootDir>/assets/vue/**/*.{js,vue}",
            "!**/specs/**"
        ]
    }
}
